<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Tata Digital Tests - Mobile (Android)" verbose="1"
	skipfailedinvocationcounts="false" junit="false" parallel="methods"
	data-provider-thread-count="1" annotations="JDK">
  <listeners>
		<listener class-name="com.qaprosoft.zafira.listener.ZafiraListener"/>
	</listeners>
	
	<parameter name="suiteOwner" value="admin" />
	<parameter name="zafira_project" value="TataDigital_Demo" />

	<test name="TCP Test">
		<classes>
			<class name="com.qaprosoft.carina.demo.TCP_CucumberTest" />
		</classes>
	</test>

</suite>
